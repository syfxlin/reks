{
  "name": "@syfxlin/reks",
  "type": "module",
  "version": "0.0.0-semantic-release",
  "description": "Keystatic extensions library.",
  "author": "Otstar Lin <i@ixk.me>",
  "license": "MIT",
  "homepage": "https://github.com/syfxlin/reks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syfxlin/reks.git"
  },
  "bugs": {
    "url": "https://github.com/syfxlin/reks/issues"
  },
  "keywords": [
    "keystatic",
    "cms"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "watch": "bunchee --watch"
  },
  "peerDependencies": {
    "@keystatic/core": "^0.3.0 || ^0.5.0",
    "@markdoc/markdoc": "^0.4.0"
  },
  "devDependencies": {
    "@keystatic/core": "^0.5.1",
    "@syfxlin/eslint-config": "^1.0.4",
    "@types/node": "^20.11.24",
    "bunchee": "^4.4.8",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  }
}
